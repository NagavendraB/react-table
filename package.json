{
  "name": "react-table",
  "version": "0.0.0",
  "description": "A sortable table component for react",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "browserify index.js -s ReactTable > ./dist/react-table.js",
    "watch": "watchify index.js -s ReactTable -o ./dist/react-table.js",
    "build:examples": "browserify   ./examples/app.js -s React > ./examples/app.built.js",
    "watch:examples": "watchify ./examples/app.js  -s React -o ./examples/app.built.js"
  },
  "author": "Nick Tomlin",
  "license": "ISC",
  "jest": {
    "testDirectoryName": "unit",
    "testPathDirs": [
      "<rootDir>/test"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/test/unit/spec-helper",
      "<rootDir>/test/unit/data"
    ]
  },
  "devDependencies": {
    "browserify": "^5.10.0",
    "connect": "^2.25.7",
    "connect-route": "^0.1.4",
    "gulp": "^3.8.7",
    "gulp-rename": "^1.2.0",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^0.3.2",
    "gulp-util": "^3.0.0",
    "jest": "^0.1.37",
    "jest-cli": "^0.1.18",
    "jshint": "^2.5.4",
    "lodash": "^2.4.1",
    "mocha": "^1.21.4",
    "morgan": "^1.2.3",
    "reactify": "^0.14.0",
    "serve-static": "^1.5.3",
    "superagent": "^0.18.2",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^1.0.2"
  }
}
